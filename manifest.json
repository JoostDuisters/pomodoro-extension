{
    "name": "Pomodoro",
    "description": "Simple chrome browser extension that serves as a timer for the pomodoro technique",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
      },
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "/images/tomato16.png",
        "32": "/images/tomato32.png",
        "64": "/images/tomato64.png",
        "128": "/images/tomato128.png"
      }
    },
    "icons": {
        "16": "/images/tomato16.png",
        "32": "/images/tomato32.png",
        "64": "/images/tomato64.png",
        "128": "/images/tomato128.png"
      }
  }